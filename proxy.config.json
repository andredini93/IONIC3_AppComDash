{
  "/gdc/*": {
    "target": "https://analytics.totvs.com.br",
    "cookieDomainRewrite": "localhost",
    "changeOrigin": true,
    "secure": false,
    "logLevel": "debug",
    "headers": {
      "host": "analytics.totvs.com.br",
      "origin": null
    }
  },
  "/*.html": {
    "changeOrigin": true,
    "cookieDomainRewrite": "localhost",
    "secure": false,
    "target": "https://analytics.totvs.com.br"
  },
  "/packages/*.{js,css}": {
    "changeOrigin": true,
    "secure": false,
    "target": "https://analytics.totvs.com.br"
  }
}